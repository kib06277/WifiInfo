<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- rectangle 表示為矩形 -->

    <stroke
        android:color="@color/black"
        android:width="2dp" />

    <!-- 填充的颜色 -->
    <solid
        android:color="@color/white" />

    <!-- android:radius 圆角的半径 -->
    <corners
        android:radius="5dp" />

</shape>